{
  "name": "microhex-js",
  "version": "0.0.1",
  "main": "./build/app.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/zenwarr/microhex-js"
  },
  "license": "MIT",
  "description": "Crossplatform hex editor intended mainly for reverse-engineering and data analysis",
  "keywords": [
    "editor",
    "hex",
    "binary",
    "microhex",
    "microhex-js",
    "zenwarr"
  ],
  "author": "Victor Tyazhelnikov",
  "engines": {
    "node": ">=6.0.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "enzyme": "^2.3.0",
    "html-webpack-plugin": "^2.21.0",
    "mocha": "^2.5.3",
    "node-sass": "^3.8.0",
    "react-addons-test-utils": "^15.1.0",
    "redux-devtools": "^3.3.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.11",
    "sass-loader": "^3.2.1",
    "style-loader": "^0.13.1",
    "ts-loader": "^0.8.2",
    "tslint": "^3.10.2",
    "typescript": "^1.8.10",
    "typings": "^1.0.0",
    "webpack": "^1.13.0"
  },
  "dependencies": {
    "async": "^2.0.0-rc.5",
    "classnames": "^2.2.5",
    "electron-prebuilt": "^1.1.0",
    "immutable": "^3.8.1",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2"
  },
  "scripts": {
    "prebuild": "typings install",
    "build": "webpack --config ./webpack/app_production.config.js && webpack --config ./webpack/client_production.config.js",
    "prebuild-dev": "typings install",
    "build-dev": "webpack --config ./webpack/app_development.config.js && webpack --config ./webpack/client_development.config.js",
    "rebuild-dev": "webpack --config ./webpack/app_development.config.js && webpack --config ./webpack/client_development.config.js",
    "start": "electron .",
    "prebuild-tests": "typings install",
    "build-tests": "webpack --config ./webpack/tests.config.js",
    "test": "mocha ./build/test/ --recursive",
    "build-and-test": "npm run build-tests && npm test"
  }
}
